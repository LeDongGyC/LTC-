@model IEnumerable<CodeFirst.Models.EF.NhanVien>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table>
    <thead>
        <tr>
            <th>STT</th>
            <th>Id</th>
            <th>Full Name</th>
            <th>Age</th>
            <th>Phong ban</th>
        </tr>
    </thead>
    @if (Model != null && Model.Any())
    {
        var i = 1;
        foreach (var item in Model)
        {
            <tr>
                <td>@i</td>
                <td>@item.Id</td>
                <td>@item.FullName</td>
                <td>@item.Age</td>
                <td>@item.PhongBan.Name</td>
            </tr>
            i++;
        }
    }

</table>